pipeline{
 agent {
     label 'master'
 }   

 tools{
     jdk 'Java'
     maven 'Maven'
 }
 
 stages{
     stage('Git clone'){
         steps{
             git branch: '${Branch}',
             url: 'https://github.com/SaiTeja-Tholeti/maven-project.git'
         }
     }
     
     stage('Build'){
         steps{
             sh 'mvn clean package'
         }
     }  
 }
}
