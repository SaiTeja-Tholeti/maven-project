pipeline{
    agent {label 'master'}
    
    tools{
        jdk 'Java'
        maven 'Maven'
    }

stages{
    stage('Git clone'){
        steps{
            git branch: 'main',
            url: 'https://github.com/SaiTeja-Tholeti/maven-project.git'
        
        }
    }
    stage('Maven build'){
        steps{
         sh 'mvn clean package'         
        }
    }


}
}
